{
  "name": "physio-dippoldiswalde",
  "description": "Physiotherapie Dippoldiswalde",
  "version": "0.0.1",
  "scripts": {
    "watch:tailwind": "postcss static/tailwind.css -o static/index.css -w",
    "build:tailwind": "NODE_ENV=production postcss static/tailwind.css -o static/index.css",
    "dev": "run-p start:dev watch:tailwind",
    "start:dev": "sapper dev",
    "build": "npm run build:tailwind && sapper build --legacy",
    "build:no-tailwind": "sapper build --legacy",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "svelte-kit": "npm uninstall @sveltejs/site-kit && npm install anito/site-kit --save-dev",
    "setup": "npm run setup:install && npm run setup:build",
    "setup:install": "pnpm install --filter @vime/core",
    "setup:build": "pnpm run build --filter @vime/core"
  },
  "dependencies": {
    "@vime/core": "^1.9.0",
    "@vime/svelte": "^1.1.1",
    "body-parser": "^1.19.0",
    "compression": "^1.7.1",
    "express-session": "1.17.1",
    "formidable": "^1.2.2",
    "node-fetch": "^2.6.0",
    "node-sass": "^4.14.1",
    "polka": "next",
    "session-file-store": "^1.4.0",
    "sirv": "^0.4.6"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/runtime": "^7.10.4",
    "@material/elevation": "^3.1.0",
    "@material/layout-grid": "^3.1.0",
    "@material/linear-progress": "^3.1.0",
    "@material/theme": "^3.1.0",
    "@material/typography": "^3.1.0",
    "@mdi/js": "^5.3.45",
    "@rollup/plugin-alias": "^3.1.0",
    "@rollup/plugin-babel": "^5.0.2",
    "@rollup/plugin-commonjs": "^12.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.3.2",
    "@smui/button": "^1.0.0-beta.21",
    "@sveltejs/site-kit": "github:anito/site-kit",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1",
    "postcss-import": "^12.0.1",
    "postcss-scss": "^2.1.1",
    "rollup": "^2.10.9",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^3.1.1",
    "rollup-plugin-svelte": "^5.2.2",
    "rollup-plugin-terser": "^6.1.0",
    "sapper": "^0.27.13",
    "serve": "^11.3.0",
    "svelte": "^3.22.3",
    "svelte-loader": "^2.13.6",
    "svelte-material-ui": "^1.0.0-beta.21",
    "svelte-preprocess": "^4.0.9",
    "tailwindcss": "^1.5.1"
  }
}
